{
  "name": "@klikkflow/cli",
  "version": "1.0.0",
  "description": "Command line interface for KlikkFlow development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./dev-tools": {
      "types": "./dist/dev-tools/index.d.ts",
      "default": "./dist/dev-tools/index.js"
    }
  },
  "bin": {
    "klikkflow": "dist/cli.js",
    "rr": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@klikkflow/core": "workspace:*",
    "@klikkflow/workflow": "workspace:*",
    "commander": "^11.1.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "boxen": "^7.1.0",
    "figlet": "^1.7.0",
    "update-notifier": "^7.0.0",
    "fs-extra": "^11.2.0",
    "execa": "^8.0.0",
    "listr2": "^7.0.0",
    "conf": "^12.0.0",
    "open": "^10.0.0",
    "semver": "^7.5.0",
    "mustache": "^4.2.0",
    "glob": "^10.3.0",
    "chokidar": "^3.5.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/figlet": "^1.5.0",
    "@types/fs-extra": "^11.0.0",
    "@types/mustache": "^4.2.0",
    "@types/semver": "^7.5.0",
    "@types/update-notifier": "^6.0.8",
    "typescript": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "@types/jest": "catalog:",
    "tsup": "catalog:"
  }
}
