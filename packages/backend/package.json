{
  "name": "@reporunner/backend",
  "version": "1.0.0",
  "description": "High-performance workflow automation backend - n8n competitor with MongoDB",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias --resolve-full-paths --resolve-full-extension .js",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:unit": "vitest src/__tests__/unit",
    "test:integration": "vitest src/__tests__/integration",
    "test:performance": "vitest src/__tests__/performance --reporter=verbose",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:unsafe": "biome check --write --unsafe .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "organize-imports": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "pnpm build && node -e \"console.log('Deploy for @reporunner/backend is not implemented yet');\"",
    "seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@reporunner/core": "workspace:*",
    "@types/pg": "^8.15.5",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.4.2",
    "googleapis": "^132.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "morgan": "^1.10.0",
    "openai": "^4.24.7",
    "p-queue": "^8.0.1",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/bcryptjs": "catalog:",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "catalog:",
    "@types/jsonwebtoken": "catalog:",
    "@types/morgan": "^1.9.9",
    "@types/node": "catalog:",
    "@types/supertest": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "supertest": "^6.3.4",
    "tsc-alias": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "workflow",
    "automation",
    "n8n",
    "competitor",
    "express",
    "typescript",
    "mongodb",
    "mongoose",
    "high-performance"
  ],
  "author": "Your Name",
  "license": "MIT"
}
