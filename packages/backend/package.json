{
  "name": "@reporunner/backend",
  "version": "1.0.0",
  "description": "High-performance workflow automation backend - n8n competitor with MongoDB",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias --resolve-full-paths --resolve-full-extension .js",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "deploy": "pnpm build && node scripts/deploy.js",
    "seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@reporunner/core": "workspace:*",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "mongoose": "^8.1.1",
    "ioredis": "^5.3.2",
    "bullmq": "^5.1.5",
    "socket.io": "^4.7.4",
    "zod": "^4.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.5",
    "p-queue": "^8.0.1",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.4.2",
    "googleapis": "^132.0.0",
    "openai": "^4.24.7",
    "@anthropic-ai/sdk": "^0.57.0",
    "express-async-errors": "^3.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "tsc-alias": "^1.8.8",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "prettier": "^3.1.1",
    "vitest": "^1.1.3",
    "@vitest/coverage-v8": "^1.1.3",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "workflow",
    "automation",
    "n8n",
    "competitor",
    "express",
    "typescript",
    "mongodb",
    "mongoose",
    "high-performance"
  ],
  "author": "Your Name",
  "license": "MIT"
}
