{
  "name": "@reporunner/ai-core",
  "version": "1.0.0",
  "description": "Core AI and ML capabilities for Reporunner",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./embeddings": {
      "types": "./dist/embeddings/index.d.ts",
      "import": "./dist/embeddings/index.mjs",
      "require": "./dist/embeddings/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.mjs",
      "require": "./dist/llm/index.js"
    },
    "./vector": {
      "types": "./dist/vector/index.d.ts",
      "import": "./dist/vector/index.mjs",
      "require": "./dist/vector/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@reporunner/core": "workspace:*",
    "@reporunner/shared": "workspace:*",
    "@anthropic-ai/sdk": "^0.27.0",
    "openai": "^4.20.0",
    "@google/generative-ai": "^0.1.0",
    "cohere-ai": "^7.0.0",
    "pg": "^8.11.0",
    "pgvector": "^0.1.0",
    "chromadb": "^1.7.0",
    "faiss-node": "^0.5.0",
    "zod": "^3.22.0",
    "tiktoken": "^1.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.10.0",
    "typescript": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "@types/jest": "catalog:",
    "tsup": "^7.0.0",
    "eslint": "catalog:"
  },
  "peerDependencies": {
    "zod": "^3.0.0"
  }
}
