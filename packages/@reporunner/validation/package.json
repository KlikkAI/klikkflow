    "@reporunner/core": "workspace:*",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "madge": "^8.0.0",
    "ora": "^8.0.1",
    "playwright": "^1.40.0",
    "typescript": "catalog:",
