{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@reporunner/core",
  "entryPoints": ["src/index.ts"],
  "out": "../../docs/api/core",
  "exclude": ["**/*+(test|spec).ts", "node_modules/**", "dist/**"],
  "excludeExternals": true,
  "excludePrivate": true,
  "categorizeByGroup": true,
  "defaultCategory": "Core",
  "readme": "README.md",
  "gitRevision": "main",
  "cleanOutputDir": true
}
