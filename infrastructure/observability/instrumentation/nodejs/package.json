{
  "name": "@reporunner/instrumentation-nodejs",
  "version": "1.0.0",
  "description": "OpenTelemetry instrumentation package for Reporunner Node.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "prepublishOnly": "pnpm build"
