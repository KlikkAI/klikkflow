{
  "dashboard": {
    "title": "Queue Metrics Dashboard",
    "tags": ["reporunner", "bullmq", "queue", "redis"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Job Processing Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(bullmq_jobs_completed_total{service=\"worker\"}[5m])",
            "legendFormat": "Completed",
            "refId": "A"
          },
          {
            "expr": "rate(bullmq_jobs_failed_total{service=\"worker\"}[5m])",
            "legendFormat": "Failed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Jobs/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "bullmq_queue_waiting{service=\"worker\"}",
            "legendFormat": "Waiting",
            "refId": "A"
          },
          {
            "expr": "bullmq_queue_active{service=\"worker\"}",
            "legendFormat": "Active",
            "refId": "B"
          },
          {
            "expr": "bullmq_queue_delayed{service=\"worker\"}",
            "legendFormat": "Delayed",
            "refId": "C"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Jobs"
          },
          {
            "format": "short"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [1000],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "name": "High Queue Depth",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Job Processing Time (p95)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(bullmq_job_duration_seconds_bucket{service=\"worker\"}[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(bullmq_job_duration_seconds_bucket{service=\"worker\"}[5m]))",
            "legendFormat": "p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "title": "Failed Jobs Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "100 * rate(bullmq_jobs_failed_total{service=\"worker\"}[5m]) / (rate(bullmq_jobs_completed_total{service=\"worker\"}[5m]) + rate(bullmq_jobs_failed_total{service=\"worker\"}[5m]))",
            "legendFormat": "Failure rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Failure Rate",
            "max": 100,
            "min": 0
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "title": "Redis Connection Pool",
        "type": "gauge",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "redis_connected_clients{job=\"redis\"}",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Redis Memory Usage",
        "type": "graph",
        "gridPos": {
          "x": 6,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "redis_memory_used_bytes{job=\"redis\"}",
            "legendFormat": "Used",
            "refId": "A"
          },
          {
            "expr": "redis_memory_max_bytes{job=\"redis\"}",
            "legendFormat": "Max",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "title": "Active Workers",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 16,
          "w": 6,
          "h": 8
        },
        "targets": [
          {
            "expr": "count(up{job=\"worker\"} == 1)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "orientation": "auto",
          "textMode": "value"
        }
      },
      {
        "id": 8,
        "title": "Job Types Distribution (Last Hour)",
        "type": "piechart",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(increase(bullmq_jobs_completed_total{service=\"worker\"}[1h])) by (job_type)",
            "legendFormat": "{{job_type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 9,
        "title": "Redis Commands/sec",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total{job=\"redis\"}[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Commands/sec"
          },
          {
            "format": "short"
          }
        ]
      }
    ]
  }
}
