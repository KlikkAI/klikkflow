{
  "annotations": { "list": [] },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "iteration": 1,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Active Jobs",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(klikkflow_queue_jobs{state=\"active\"})",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Pending Jobs",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "sum(klikkflow_queue_jobs{state=\"waiting\"})",
          "legendFormat": "Pending",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Failed Jobs (24h)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(increase(klikkflow_queue_jobs_failed_total[24h]))",
          "legendFormat": "Failed",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Queue Processing Rate",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(klikkflow_queue_jobs_completed_total[5m]))",
          "legendFormat": "Jobs/s",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Depth by State",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "klikkflow_queue_jobs",
          "legendFormat": "{{state}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Job Processing Time",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(klikkflow_queue_job_duration_seconds_bucket[5m])) by (le, queue))",
          "legendFormat": "p50 {{queue}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(klikkflow_queue_job_duration_seconds_bucket[5m])) by (le, queue))",
          "legendFormat": "p95 {{queue}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(klikkflow_queue_job_duration_seconds_bucket[5m])) by (le, queue))",
          "legendFormat": "p99 {{queue}}",
          "refId": "C"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Job Completion vs Failure Rate",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(klikkflow_queue_jobs_completed_total[5m]))",
          "legendFormat": "Completed",
          "refId": "A"
        },
        {
          "expr": "sum(rate(klikkflow_queue_jobs_failed_total[5m]))",
          "legendFormat": "Failed",
          "refId": "B"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Worker Utilization",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "klikkflow_queue_workers_active / klikkflow_queue_workers_total * 100",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Wait Time",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(klikkflow_queue_job_wait_duration_seconds_bucket[5m])) by (le, queue))",
          "legendFormat": "p95 {{queue}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": { "mode": "palette-classic" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 30 },
              { "color": "red", "value": 300 }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Retry Attempts",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(klikkflow_queue_job_retries_total[5m])) by (queue)",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "type": "table",
      "title": "Queue Statistics",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "klikkflow_queue_jobs",
          "legendFormat": "{{queue}} - {{state}}",
          "refId": "A",
          "instant": true,
          "format": "table"
        }
      ],
      "datasource": { "type": "prometheus", "uid": "PROM_DS" },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true },
            "indexByName": { "queue": 0, "state": 1, "Value": 2 },
            "renameByName": { "queue": "Queue", "state": "State", "Value": "Count" }
          }
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 37,
  "style": "dark",
  "tags": ["klikkflow", "queue", "workers", "jobs"],
  "templating": { "list": [] },
  "time": { "from": "now-6h", "to": "now" },
  "timezone": "",
  "title": "KlikkFlow Queue Metrics",
  "uid": "klikkflow-queue-metrics",
  "version": 1
}
